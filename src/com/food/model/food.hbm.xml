<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.food.model.FoodVO" table="FOOD" >
        
        <id name="food_ID" column="FOOD_ID">
            <generator class="com.genterator.CookGodGenerator"/>
        </id>
        
        
        <property name="food_name" column="FOOD_NAME" />
        <property name="food_type_ID" column="FOOD_TYPE_ID" />
        
    </class>
</hibernate-mapping>